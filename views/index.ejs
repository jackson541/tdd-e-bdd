<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Nome</th>
          <th scope="col">Telefone</th>
        </tr>
      </thead>
      <tbody>
        <% for(let cliente of clientes) { %>
          <tr>
            <th scope="row"><%= cliente.id %></th>
            <td><%= cliente.nome %></td>
            <td><%= cliente.telefone %></td>
          </tr>
        <% } %>
      </tbody>
    </table>

  </body>
</html>
